{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mannco.store API",
  "description": "Complete API documentation for Mannco.store - Buy and sell TF2, CS:GO, Dota 2 and Rust skins on the cheapest secured marketplace.",
  "colors": {
    "primary": "#29CCE0",
    "light": "#29CCE0",
    "dark": "#29CCE0"
  },
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "background": {
    "color": {
      "light": "#021214",
      "dark": "#021214"
    },
    "decoration": "gradient"
  },
  "favicon": "/favicon.svg",
  "seo": {
    "indexing": "all"
  },
  "metadata": {
    "og:site_name": "Mannco.store API Documentation",
    "og:type": "website",
    "og:locale": "en_US",
    "twitter:card": "summary_large_image",
    "twitter:site": "@manncostore"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Authentication",
            "pages": [
              "api-reference/authentication"
            ]
          },
          {
            "group": "Items",
            "pages": [
              "api-reference/items",
              "api-reference/inventory",
              "api-reference/bundles"
            ]
          },
          {
            "group": "Trading",
            "pages": [
              "api-reference/trades",
              "api-reference/offers",
              "api-reference/buyorders"
            ]
          },
          {
            "group": "Payments",
            "pages": [
              "api-reference/deposit",
              "api-reference/cart",
              "api-reference/payment",
              "api-reference/gift",
              "api-reference/cashout"
            ]
          },
          {
            "group": "User Account",
            "pages": [
              "api-reference/user-account",
              "api-reference/user-settings",
              "api-reference/user-2fa",
              "api-reference/user-email",
              "api-reference/user-history"
            ]
          },
          {
            "group": "Cashout & Payment",
            "pages": [
              "api-reference/cashout",
              "api-reference/payment"
            ]
          },
          {
            "group": "Utilities & Public Data",
            "pages": [
              "api-reference/utils",
              "api-reference/public-data"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://mannco.store"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Mannco.store",
      "href": "https://mannco.store"
    }
  }
